#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() 
{
    int a[1000],i,n,c=1;
    scanf("%d", &n);
    for(i=0;i<n;i++)
        scanf("%d", &a[i]);
    for(i=0;i<n;i++)
    {
        while(a[i]==8&&a[i+1]==8)
        {
            i++;
            c++;
        
        }
        
    }
    printf("%d", c);
    return 0;
}
